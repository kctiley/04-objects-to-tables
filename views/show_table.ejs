<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <table>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Date of Birth</th>
      </tr>
      <% for (var i = 0; i < data.length; i++){ %>
        <tr>
          <td><%= data[i].first_name %></td>
          <td><%= data[i].last_name %></td>
          <td><%= data[i].date_of_birth %></td>
        </tr>
      <% } %>
    </table>
  </body>
</html>